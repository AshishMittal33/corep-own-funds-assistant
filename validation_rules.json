        "error_message": "CET1 calculation inconsistency",
        "severity": "ERROR"
      },
      {
        "rule_id": "CET1_MINIMUM",
        "description": "CET1 must be at least 4.5% of RWA",
        "condition": "100 >= rwa * 0.045",
        "error_message": "CET1 below minimum regulatory requirement",
        "severity": "ERROR"
      }
    ],
    "completeness_rules": [
      {
        "required_fields": ["010", "020", "030", "100"],
